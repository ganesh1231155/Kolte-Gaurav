#QUE-1
> a=c(7,18,-2,11,-18,3,33,1,9,-4,0,21) ;a
 [1]   7  18  -2  11 -18   3  33   1   9  -4   0  21
> A=matrix(a,byrow=T,ncol=4) ;A
     [,1] [,2] [,3] [,4]
[1,]    7   18   -2   11
[2,]  -18    3   33    1
[3,]    9   -4    0   21
> A1=t(X) ;A1
     [,1] [,2] [,3]
[1,]    7  -16    9
[2,]   18    3   -4
[3,]   -2   55    0

#QUE-2i)
> a=c(2,4,5,9,3,8) ;a
[1] 2 4 5 9 3 8
> b=c(4,9) ;b
[1] 4 9
> A=matrix(a,byrow=T,ncol=2) ;A
     [,1] [,2]
[1,]    2    4
[2,]    5    9
[3,]    3    8
> B=matrix(b,byrow=T,ncol=1) ;B
     [,1]
[1,]    4
[2,]    9
> M=A%*%B ;M
     [,1]
[1,]   44
[2,]  101
[3,]   84

#QUE-2ii)
> a=c(12,4,5,8,23,18) ;a
[1] 12  4  5  8 23 18
> b=c(32,14,5,7,3,8) ;b
[1] 32 14  5  7  3  8
> A=matrix(a,byrow=T,ncol=2) ;A
     [,1] [,2]
[1,]   12    4
[2,]    5    8
[3,]   23   18
> B=matrix(b,byrow=T,ncol=2) ;B
     [,1] [,2]
[1,]   32   14
[2,]    5    7
[3,]    3    8
> Add=A+B ;Add
     [,1] [,2]
[1,]   44   18
[2,]   10   15
[3,]   26   26


