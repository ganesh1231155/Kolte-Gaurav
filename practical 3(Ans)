#Practical No. 3

> x=c(1,3,7,11,6) ;x
[1]  1  3  7 11  6
> y=c(1:5) ;y
[1] 1 2 3 4 5
> A=cbind(x,y) ;A
      x y
[1,]  1 1
[2,]  3 2
[3,]  7 3
[4,] 11 4
[5,]  6 5
> z=c(x,y) ;z
 [1]  1  3  7 11  6  1  2  3  4  5
> A=matrix(z,byrow=F,ncol=2) ;A
     [,1] [,2]
[1,]    1    1
[2,]    3    2
[3,]    7    3
[4,]   11    4
[5,]    6    5

#Diagonal Matrix
> A1=diag(c(5,20,10)) ;A1
     [,1] [,2] [,3]
[1,]    5    0    0
[2,]    0   20    0
[3,]    0    0   10

#Transpose Matrix
> A2=t(A) ;A2
     [,1] [,2] [,3] [,4] [,5]
[1,]    1    3    7   11    6
[2,]    1    2    3    4    5

#Scalar Multiplication
> A3=5*A1 ;A3
     [,1] [,2] [,3]
[1,]   25    0    0
[2,]    0  100    0
[3,]    0    0   50

#Matrix Addition
> b1=c(2,4,5,9,3,8) ;b1
[1] 2 4 5 9 3 8
> b2=c(3,4,5,10,39,8) ;b2
[1]  3  4  5 10 39  8
> B1=matrix(b1,byrow=T,ncol=2) ;B1
     [,1] [,2]
[1,]    2    4
[2,]    5    9
[3,]    3    8
> B2=matrix(b2,byrow=T,ncol=2) ;B2
     [,1] [,2]
[1,]    3    4
[2,]    5   10
[3,]   39    8
> B3=B1+B2 ;B3
     [,1] [,2]
[1,]    5    8
[2,]   10   19
[3,]   42   16

#Matrix Multiplication
> B4=matrix(c(2,4,3,7),byrow=T,ncol=2) ;B4
     [,1] [,2]
[1,]    2    4
[2,]    3    7
> B3%*%B4
     [,1] [,2]
[1,]   34   76
[2,]   77  173
[3,]  132  280

#Determinant Of Matrix
> det(B4)
[1] 2

#Inverse Of Matrix
> B5=solve(B4) ;B5
     [,1] [,2]
[1,]  3.5   -2
[2,] -1.5    1
> round(B5)
     [,1] [,2]
[1,]    4   -2
[2,]   -2    1


